<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

<table class="table table-striped table-dark">
  <br><br> <br>
  <thead>
    <tr>
       
      <th scope="col">Category Name</th>
      <th scope="col">Image</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    
      <% records.forEach(function(img2){ %>
    <tr>
        
	<td><%=img2.name %></td>
        <td><img src="/upload/<%= img2.image %>" alt="image" style="width: 100px;"></td>
    
	<td>
	   
          <a class="btn btn-danger" href="/add_edit/<%= img2._id%>">edit</a>  
          <a class="btn btn-success" href="/category_delete/<%= img2._id%>">delete</a>
        </td>	
</tr>
      <%})%>
  </tbody>
</table>










